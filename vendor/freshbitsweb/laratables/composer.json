{
    "name": "freshbitsweb/laratables",
    "description": "Ajax support of DataTables for Laravel 5.5+",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0.0",
        "phpunit/phpunit": "^9.3.9",
        "laravel/legacy-factories": "^1.0.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\Laratables\\LaratablesServiceProvider"
            ]
        }
    }
}
